@article{casterline_infant_1992,
	title = {Infant and child mortality in rural Egypt},
	volume = {24},
	issn = {1469-7599, 0021-9320},
	url = {https://www.cambridge.org/core/journals/journal-of-biosocial-science/article/infant-and-child-mortality-in-rural-egypt/AE8450B1436E49092033AC2A71597F53},
	doi = {10.1017/S0021932000019763},
	abstract = {This research examines determinants of infant and child mortality in rural Egypt, primarily the effects of household economic status and the availability of health services. Certain features of the health service environment affect survival in the neonatal period. In early childhood, survival chances improve markedly as income increases and if the household depends almost exclusively on employment income. In infancy and in early childhood, mortality is strongly associated with region of residence and maternal demographic characteristics, and is weakly associated with parental schooling.},
