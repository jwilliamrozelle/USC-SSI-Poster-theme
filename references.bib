@inproceedings{graschew_global_2010,
	title = {Global healthcare in the 21st century: {ICT}’s and the virtualisation of hospitals},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85092067134&partnerID=40&md5=f8854ad87484910a9c1333856c7df515},
	abstract = {Ubiquitous access to high-level healthcare (u-Health) requires increasing use of Information and Communication Technology ({ICT}) solutions. A regional cooperation with leading hospitals can be supported by the integration of technological platforms and telemedical services in a Virtual Hospital. By implementing interactive real-time connections with the high-end video communication system {WinVicos} live interaction with the remote partner becomes possible. Such a permanent link enables services like expert advice for patient care as well as teleteaching, teletraining and telementoring of health professionals. It is necessary to not only focus on the technology but to address the users’ needs by providing a transparent platform and strategies for collaboration in virtual user communities. © 2010 World Telecommunications Congress 2010, {WTC} 2010. All rights reserved.},
