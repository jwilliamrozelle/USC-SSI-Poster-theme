	author = {Gelman, Andrew and Carlin, John B. and Stern, Hal S. and Dunson, David B. and Vehtari, Aki and Rubin, Donald B.},
	date = {2014},
	keywords = {Bayesian statistical decision theory, {MATHEMATICS} / Probability \& Statistics / General},
}

