@article{moyer_facility-based_2013,
	title = {Facility-based delivery and maternal and early neonatal mortality in sub-Saharan Africa: A regional review of the literature},
	volume = {17},
	rights = {Copyright (c)},
	issn = {1118-4841},
	url = {https://www.ajol.info/index.php/ajrh/article/view/93744},
