	langid = {english},
	note = {Publisher: Public Library of Science},
	keywords = {{ANC}, Antenatal care, {DHS}, Donabedian, Indonesia, Labor and delivery, Neonatal care, {PNC}, Pregnancy, Principal component analysis, Quality of care, Tetanus},
}

